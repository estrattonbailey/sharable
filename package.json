{
  "name": "sharable",
  "version": "0.0.1",
  "description": "An easily configurable social share libary that uses the social data in your document's head by default.",
  "main": "dist/sharable.js",
  "scripts": {
    "package": "mkdir browser ; browserify index.js --standalone overunder > browser/overunder.js"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/sharable.git"
  },
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/sharable/issues"
  },
  "homepage": "https://github.com/estrattonbailey/sharable#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "minifyify": "^7.3.3",
    "watchify": "^3.7.0"
  }
}
